<Window x:Class="Mahhong.HallofFame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mahhong"
        mc:Ignorable="d"
        Title="HallofFame" Height="616.071" Width="690.179" Icon="mahjongicono.png" ResizeMode="NoResize">
    <DataGrid ItemsSource="{Binding ListadoJugadores}" AutoGenerateColumns="False">
    <DataGrid.Columns>
        <DataGridTextColumn Header="posicion" Binding="{Binding posicion}"/>
        <DataGridTextColumn Header="nombre" Binding="{Binding nombre}"/>
        <DataGridTextColumn Header="tiempo" Binding="{Binding tiempo}"/>
     </DataGrid.Columns>
    </DataGrid>
</Window>
